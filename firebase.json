{
  "firestore": {
    "rules": "firestore.rules"
  },
  "storage": {
    "rules": "storage.rules",
    "bucket": "autocaravana-reservas.firebasestorage.app"
  },
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/login",
        "destination": "/login.html"
      },
      {
        "source": "/reservar",
        "destination": "/reservar.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
